		"svelte": "^4.0.5",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.5",
		"typescript": "^5.0.0",
		"vite": "^4.5.0"
	},
	"type": "module",
	"dependencies": {
		"cors": "^2.8.5",
		"express": "^4.18.2",
		"mongoose": "^8.0.1",
